# 环境配置
NODE_ENV=development

# 数据库配置
# 方式1: 使用 DATABASE_URL (推荐)
# DATABASE_URL=postgres://username:password@host:port/database

# 方式2: 使用单独的变量 (Clacky 中间件自动提供)
# POSTGRE_SQL_INNER_HOST=127.0.0.1
# POSTGRE_SQL_INNER_PORT=5432
# POSTGRE_SQL_USER=postgres
# POSTGRE_SQL_PASSWORD=your_password_here
# POSTGRE_SQL_DATABASE=postgres

# 服务器配置
PORT=5173